<html>
  <head>
    <style>
      body {
         font-family: 'Segoe UI', Tahoma, sans-serif;
      }

		.grad {
			background: white;
		}

    .button:hover {
      background-color:black;
      box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24), 0 17px 50px 0 rgba(0,0,0,0.19);
    }

    .button {
      background-color: #555555;
      -webkit-transition-duration: 0.4s; /* Safari */
      transition-duration: 0.4s;
      border: none;
      border-radius: 10px;
      color: white;
      padding: 10px 10px;
      text-align: center;
      text-decoration: none;
      display: inline-block;
      font-family: 'Segoe UI', Tahoma, sans-serif;
    }
	
	h1 {
		color: #333;
		font-size: 1.6em;
		font-weight: normal;
		line-height: 1.25em;
		margin-bottom: 16px;
		}

	h2 {
		font-size: 1em;
		line-height: 1.6em;
		font-weight: normal;
	}
	
	strong {
		font-weight: 500;
	}
	
    </style>
	<title>Website Redirected</title>
  </head>
  <body class="grad" style="margin:100;padding:0" onload="onLoad();">
  <script>
    REDIRECT_URL = "http://100.127.255.255/redirect-url=";

    function checkTimeElapsedSinceLastRedirect()
    {
        var currUrl = window.location.href.substring(window.location.protocol.length);
        var prevTime = localStorage[currUrl];
        var currTime = new Date().getTime()
        localStorage[currUrl] = currTime;
        
        if ((currTime - prevTime > 10000) || (prevTime === undefined))
        {
            return true;
        }
        return false;
    }
    
    function onLoad() {
        var currUrl = window.location.href;
        if (currUrl.indexOf(REDIRECT_URL) == 0) {
          document.getElementById("mainBody").style.display = 'block';
          var requestAccessElement = document.getElementById("requestAccessButton");
          requestAccessElement.parentNode.removeChild(requestAccessElement);
          document.getElementById("requestAccessText").innerText = "You will now be redirected";
          return;
        }
        
        if (location.protocol === 'http:')
        {
            if (checkTimeElapsedSinceLastRedirect())
            {
                location.href = 'https:' + window.location.href.substring(window.location.protocol.length);
                return;
            }
        }
        
        document.getElementById("mainBody").style.display = 'block';
        
        var currUrl = window.location.href;
        if (currUrl.indexOf(REDIRECT_URL) == 0) {
          var requestAccessElement = document.getElementById("requestAccessButton");
          requestAccessElement.parentNode.removeChild(requestAccessElement);
          document.getElementById("requestAccessText").innerText = "You will now be redirected";
          return;
        }

        if (!inIframe()) {
          allowUrl();
        }
    }

    function allowUrl() {
        var currUrl = window.location.href;
        var res = encodeURIComponent(currUrl);
        var newUrl = REDIRECT_URL + res;
        window.location.href = newUrl;
    }

    function inIframe () {
      try {
          return window.self !== window.top;
      } catch (e) {
          return true;
      }
    }
  </script>
  
      <div style="margin-left:20%; margin-top:10%; width:600px;">
	
		<img style="width:70px;" alt="Embedded Image" src="data:image/svg+xml;utf8;base64," />
		<h1>The requested website is being redirected. </h1>
		<h2>Your browser attempted to access a website that is not available in this environment:<strong><div id='url-placeholder'/> </h2>
		<h2>The website will be launched in another environment. </h2>
		<button class="button" onclick="allowUrl();" id="requestAccessButton">Launch Website</button>
	</div>
  </body>
</html>

# Check Backward Compatibility with Squid-3 formats
external_acl_type foo \
	%LOGIN \
	%un \
	%EXT_USER \
	%EXT_LOG \
	%EXT_TAG \
	%IDENT \
	%SRC \
	%SRCPORT \
	%URI \
	%DST \
	%PROTO \
	%PORT \
	%PATH \
	%METHOD \
	%MYADDR \
	%MYPORT \
	%PATH \
	%>{Header} \
	%>{Hdr:member} \
	%>{Hdr:;member} \
	%>{Hdr:Xmember} \
	%<{Header} \
	%<{Hdr:member} \
	%<{Hdr:;member} \
	%<{Hdr:Xmember} \
	%ACL \
	%DATA \
	%%  /bin/true

